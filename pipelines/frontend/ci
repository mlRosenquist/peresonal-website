node('nodejs') {

    stage('Clone repository') {
        checkout scm
    }

    stage('Node.js setup1') {
        dir('frontend'){
            sh 'node -v'
            sh 'yarn -v'
            sh 'yarn'
        }
    }
    
    stage('Build project') {
        dir('frontend'){
            sh 'yarn build'
        }
    }

    stage('Test Project') {

    }
}